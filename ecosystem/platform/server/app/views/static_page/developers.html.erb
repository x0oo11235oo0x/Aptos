            class="grid grid-cols-1 lg:grid-cols-2 gap-4 lg:gap-8 justify-center mt-10">
          <%= render ButtonComponent.new(href: 'https://github.com/aptos-labs', size: :large, class: 'w-full', title: 'Aptos on GitHub') do %>
            GitHub
          <% end %>
          <%= render ButtonComponent.new(href: 'https://aptos.dev/', size: :large, class: 'w-full', title: 'Aptos Docs') do %>
            Documentation
          <% end %>
          <%= render ButtonComponent.new(href: 'https://explorer.devnet.aptos.dev/', size: :large, class: 'w-full', title: 'Aptos Explorer') do %>
            Explorer
          <% end %>
          <%= render ButtonComponent.new(href: 'https://discord.gg/aptoslabs', size: :large, class: 'w-full', title: 'Aptos on Discord') do %>
            Discord
          <% end %>
        </div>
      </div>
