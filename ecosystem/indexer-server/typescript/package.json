{
  "name": "typescript-indexer",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/index",
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "generate": "npm -s run generate:prisma && npx openapi-typescript ../doc/openapi.yaml --output src/openapi-schema.ts",
    "generate:openapi-express": "java -jar openapi-generator-cli-5.4.0.jar generate -g nodejs-express-server -i ../doc/openapi.yaml -o src/openapi-express",
    "generate:prisma": "prisma generate",
    "generate:openapi": "npx openapi-typescript ../doc/openapi.yaml --output src/openapi-schema.ts",
    "test": "jest",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.1",
    "@prisma/client": "3.14.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/js-yaml": "^4.0.5",
    "@types/swagger-ui-express": "^4.1.3",
    "body-parser": "^1.19.0",
    "camelcase": "^5.3.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "express": "4.18.1",
    "express-openapi-validator": "^4.13.8",
    "js-yaml": "^4.1.0",
    "ono": "^5.0.1",
    "openapi-sampler": "^1.0.0-beta.15",
    "path-to-regexp": "^6.2.1",
    "swagger-ui-express": "^4.0.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "16.11.36",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "axios": "^0.27.2",
    "eslint": "^8.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-sort-class-members": "^1.14.1",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "jest": "^27.5.1",
    "prisma": "3.14.0",
    "ts-jest": "^27.1.4",
    "ts-node": "10.7.0",
    "ts-node-dev": "1.1.8",
    "typescript": "4.6.4"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
